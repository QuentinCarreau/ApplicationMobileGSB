<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    
    <Button 
        android:id="@+id/wBgeoloc"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:text="@string/Bgeoloc"
	    android:onClick="wBgeoloc_onClick"
        />
    
    <Button 
        android:id="@+id/wBbook"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:text="@string/Bbook"
	    android:onClick="wBbook_onClick"
        />
    
    <Button 
        android:id="@+id/wBcompany"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:text="@string/Bcompany"
	    android:onClick="wBcompany_onClick"
        />

</LinearLayout>
